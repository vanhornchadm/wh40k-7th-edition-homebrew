        <profile id="6539-2da8-c4c6-2815" name="The Ardent Blade (Melee)" publicationId="818a0a0f--pubN68426" page="0" hidden="false" typeId="d5f97c0b-9fc9-478d-aa34-a7c414d3ea48" typeName="Weapon">
