      <profiles>
        <profile id="69df-50e3-dffe-8fdc" name="Mastodon" publicationId="6cacb5fc--pubN172484" hidden="false" typeId="725a358c-765b-498c-8de5-399fc0c0725f" typeName="Vehicle">
