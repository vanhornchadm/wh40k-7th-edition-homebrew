                <profile id="ac05-f08c-55e5-5387" name="Warlord Battle Titan" hidden="false" typeId="3dadd2ff-33f1-41dd-85c7-bee5a7dfa413" typeName="Walker">
                  <characteristics>
                    <characteristic name="WS" typeId="5ee4ff0b-b244-4670-9d05-91d10f80c32e">
