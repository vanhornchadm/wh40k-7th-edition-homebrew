    <profile id="5e3f-f90b-456b-0765" name="Pintle-Mounted Storm Bolter" publicationId="7ab0-ceb9-pubN65751" hidden="false" typeId="d5f97c0b-9fc9-478d-aa34-a7c414d3ea48" typeName="Weapon">
