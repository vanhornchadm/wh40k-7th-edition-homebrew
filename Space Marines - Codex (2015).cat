    <selectionEntry id="75e6-8784-c242-50de" name="Ion Shield" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="parent" value="1" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="2722-a4b3-193a-e132" type="min"/>
