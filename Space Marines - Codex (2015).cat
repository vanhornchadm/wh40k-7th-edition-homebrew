    <selectionEntry id="d769-1321-8c9a-46c4" name="(FW) Mastodon Heavy Assault Transport" publicationId="7ab0-ceb9-pubN218282" hidden="false" collective="false" import="true" type="model">
      <profiles>
        <profile id="1907-1282-8dc7-af50" name="Mastodon" publicationId="7ab0-ceb9-pubN174565" hidden="false" typeId="725a358c-765b-498c-8de5-399fc0c0725f" typeName="Vehicle">
          <characteristics>
