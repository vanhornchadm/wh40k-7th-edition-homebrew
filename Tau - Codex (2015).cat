      <rules>
        <rule id="e405-c162-9e08-680d" name="Guerilla Fighters" hidden="false">
          <description>Vespid Stingwing units from this formation gain Infiltrate and Stealth (Forests) special rules.</description>
        </rule>
        <rule id="78cb-1f8d-4259-ba67" name="Strategic Intel" hidden="false">
